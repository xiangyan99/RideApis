      <Author>Microsoft Corporation</Author>
      <SnippetTypes>
        <SnippetType>SurroundsWith</SnippetType>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>true</ID>
          <Default>true</Default>
        </Literal>
      </Declarations>
      <Code Language="Nodejs">
        <![CDATA[if ($true$) {
    $selected$$end$
}
]]>
