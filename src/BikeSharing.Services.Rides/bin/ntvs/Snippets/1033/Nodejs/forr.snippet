<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets
    xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>forr</Title>
      <Description>Code Snippet for a reverse For Loop</Description>
      <Shortcut>forr</Shortcut>
      <Author>Microsoft Corporation</Author>
      <SnippetTypes>
        <SnippetType>SurroundsWith</SnippetType>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>length</ID>
          <Default>length</Default>
        </Literal>
        <Literal>
          <ID>i</ID>
          <Default>i</Default>
        </Literal>
      </Declarations>
      <Code Language="Nodejs">
        <![CDATA[for (var $i$ = $length$ - 1; $i$ >= 0; $i$--) {
    $selected$$end$
};
]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
