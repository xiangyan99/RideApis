<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets
    xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>express_put</Title>
      <Description>Code Snippet for handling a PUT request with Express</Description>
      <Shortcut>expput</Shortcut>
      <Author>Microsoft Corporation</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>Hello World</ID>
          <Default>Hello World</Default>
        </Literal>
        <Literal>
          <ID>route</ID>
          <Default>route</Default>
        </Literal>
      </Declarations>
      <Code Language="Nodejs">
        <![CDATA[app.put('$route$', function (request, response) {
    response.send('$Hello World$');$end$
});
]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
